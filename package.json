{
  "name": "image-processing-api",
  "version": "1.0.0",
  "description": "An api to return resized images efficently",
  "main": "dist/index.js",
  "scripts": {
    "start": "nodemon --watch './**/*.ts' --exec \"ts-node\" ./src/index.ts",
    "build": "npx tsc",
    "start:prod": "npm run build && node dist/index.js",
    "lint": "eslint .",
    "lint:fix": "eslint --fix",
    "format": "prettier --write src/**/*.ts",
    "test": "npx tsc && jasmine"
  },
  "author": "Youssef Dawoud",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jasmine": "^3.10.3",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.21",
    "@types/sharp": "^0.29.5",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jasmine": "^4.0.2",
    "nodemon": "^2.0.15",
    "prettier": "2.5.1",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "jasmine-spec-reporter": "^7.0.0",
    "morgan": "^1.10.0",
    "sharp": "^0.30.2",
    "supertest": "^6.2.2"
  }
}
